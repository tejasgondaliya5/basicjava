public class Main {
  public static void main(String[] args) {
    int[] no = {0, 1, 2, 3, 4, 5};
    {
      System.out.println(no[0]);
      System.out.println(no[1]);
      System.out.println(no[2]);
      System.out.println(no[3]);
      for(int i : no){
      System.out.println(i);
      }
    }    
  }
}


